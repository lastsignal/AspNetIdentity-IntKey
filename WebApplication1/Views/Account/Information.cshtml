@using System.Security.Claims
@{
    ViewBag.Title = "Account Information";
}

<h2>Account Information</h2>

<table>
    <caption>Claims</caption>
    <tr>
        <th>Type</th>
        <th>Value</th>
    </tr>
    @foreach (var claim in ((ClaimsIdentity) User.Identity).Claims)
    {
    <tr>
        <td>@claim.Type</td>
        <td>@claim.Value</td>
    </tr>
    }
</table>
